{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "clean": "rimraf _site",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "dev:tailwind": "npx tailwindcss -i style/tailwind.css -o _site/css/tw.css --watch",
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "build:tailwind": "npx tailwindcss -i style/tailwind.css -o _site/css/tw.css --minify",
    "build:eleventy": "ELEVENTY_PRODUCTION=true npx @11ty/eleventy",
    "dev": "npm-run-all clean --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean build:eleventy build:tailwind"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Geshan Manandhar",
    "email": "geshan@gmail.com",
    "url": "https://geshan.com.np/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^v2.0.0-canary.11",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "axios": "^0.27.2",
    "eleventy-google-fonts": "^0.1.0",
    "eleventy-plugin-blog-tools": "^0.3.1",
    "html-minifier": "^4.0.0",
    "luxon": "^2.4.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.4",
    "workbox-build": "^6.5.3"
  }
}
