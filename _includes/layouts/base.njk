<!doctype html>
<html class="no-js" lang="en">
  <head>
    {%- include "components/meta.njk" -%}
    {%- include "components/favicons.njk" -%}
     {%- include "components/fonts.njk" -%}
    <link href="/atom.xml" rel="alternate" title="{{ metadata.title }}" type="application/atom+xml">
    <title>{{ pagetitle or renderData.title or title or metadata.title }}</title>

    <link rel="stylesheet" href="{{ '/css/tailwind.css' | url }}">
   
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    {%- include "components/ga.njk" -%}
    {%- include "components/sw.njk" -%}
  </head>
  <body class="overflow-x-hidden font-leading">
    {%- include "components/gtm.njk" -%}
    {%- include "components/header.njk" -%}

    <a href="#content" class="sr-only sr-only-focusable">Skip to main content</a>
    <div id="wrap">
      <header role="banner">
        {%- include "components/avocado-nav.njk" -%}
      </header>

      <div id="main" {% if templateClass %} class="{{ templateClass }} container"{% endif %}>
        <div id="content">
          <div class="row">
            <div class="page-content col-md-8">
                {{ content | safe }}
            </div>
            <aside class="sidebar col-md-4">
                {% include "sidebar/latest-posts.njk" %}
                {% include "sidebar/read-more-on.njk" %}
                {% include "sidebar/carbon.njk" %}
                {% include "sidebar/side-projects.njk" %}
                {% include "sidebar/subscribe-nl.njk" %}
            </aside>
        </div><!-- div class row close -->
      </div><!-- div id content close -->
      </div>
    </div><!-- wrap close -->
    {% include "footer/footer.njk" %}
    <!-- Current page: {{ page.url | url }} -->
    <script src="/js/all.min.js" defer></script>
  </body>
</html>
