        {%- include "components/avocado-nav.njk" -%}
      </header>
      <div id="main">
        <div id="content">
          <div class="row">
            <div>
              {{ content | safe }}
            </div>
            {% if page.url == '/' %}
            <!-- section popular topics -->
            <section>
              <div class="mt-24 max-w-6xl mx-auto md:px-4 px-4 sm:px-4">
                <div class="grid-cols-12 grid">
                  <div class="col-span-12 md:col-span-7">
                    <h2 class="font-leading font-bold text-5xl text-darkpurple">
                      Popular topics
                    </h2>
                    <div class="mt-12 flex flex-wrap">
                      <span class=" background-lightbg mr-4 mb-4 rounded border-2 bg-gray-100 font-semibold text-darkgrey hover:text-purple px-3 py-1
                          text-xl border-gray-300 cursor-pointer hover:border-gray-400 ">
                        nodejs</span>
                      <span class=" background-lightbg mr-4 mb-4 rounded border-2 bg-gray-100 font-semibold text-darkgrey hover:text-purple px-3 py-1
